   ],
   "source": [
    "#Creating a Correlation heatmap for better understanding relationship between label and features\n",
    "plt.figure(figsize=(10, 6))\n",
    "sns.set_theme(style=\"white\")\n",
    "corr = df.corr()\n",
    "sns.heatmap(corr, annot=True, cmap=\"Blues\", fmt=\".1g\")"
   ]
  },
